{
  "expo": {
    "name": "Car-Clone",
    "slug": "uber-2",
    "version": "1.0.0",

    "web": {
      "bundler": "metro"
    },

    "experiments": {
      "tsconfigPaths": true
    },

    "plugins": [],

    "orientation": "portrait",
    "icon": "./app/assets/icons/icon",
    "userInterfaceStyle": "light",

    "splash": {
      "image": "./app/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icons/icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
